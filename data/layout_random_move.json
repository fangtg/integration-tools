{
	"frame_path": {
		"line_scan": {
			"scan_path_button": {
				"type": "Button",
				"text": "输入目录",
				"key": "scan_path",
				"command": "self.select_folder"
			},
			"scan_path_entry": {
				"type": "Entry",
				"width": "50",
				"key": "scan_path"
			}
		},
		"line_out": {
			"out_path_button": {
				"type": "Button",
				"text": "输出目录",
				"key": "out_path",
				"command": "self.select_folder"
			},
			"out_path_entry": {
				"type": "Entry",
				"width": "50",
				"key": "out_path"
			}
		}
	},
	"frame_configuration": {
		"line_random_ratio": {
			"random_ratio_label": {
				"type": "Label",
				"text": "随机比例"
			},
			"random_ratio_entry": {
				"type": "Entry",
				"width": "5",
				"key": "random_ratio"
			},
			"mark_type_label": {
				"type": "Label",
				"text": "对应标注类型"
			},
			"mark_type_radiobutton": {
				"type": "Radiobutton",
				"key": "mark_type",
				"value": [".xml", ".json"],
				"direction": "left"
			}
		}
	},
	"frame_button": {
		"line_1": {
			"confirm_button": {
				"type": "Button",
				"text": "确认",
				"key": "confirm",
				"command": "self.confirm"
			}
		}
	},
	"frame_progress_bar": {
		"line1": {
			"progressbar": {
				"type": "Progressbar",
				"key": "progress_bar",
				"width": 500
			}
		}
	}
}